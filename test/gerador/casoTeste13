new robot misterRobot13
    
    defop _addAbs a as integer, b as integer returns integer
        + a b -> 
        If returns integer
            Is lesser 0 - (+ a b) -> Out
            Default + a b -> Out
        -> Out
	
    defcom Init
        Let a be integer
        Let b be integer
        Assign 41 a
        Assign 42 b
        "|" -> + a -> + " " -> + b -> + "|" 
        -> + " = " -> + (_addAbs a b) -> Print

end
