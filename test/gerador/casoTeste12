new robot misterRobot12
    
	defop _addAbs a as integer, b as integer returns integer
        + (If a returns integer
            Is lesser 0 Out (- a)
            Default Out a)
        (If b returns integer
            Is lesser 0 Out (- b)
            Default Out b) -> Out
	
    defcom Init
        Let a be integer
        Let b be integer
        Assign 41 a
        Assign 42 b
        (_addAbs a b) -> Print

end
